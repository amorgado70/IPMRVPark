@{
    ViewBag.Title = "Staff Log In";
}

@Html.ActionLink("Home", "Home", "PrototypeUI")
<span class="glyphicon glyphicon-chevron-right"></span>
@Html.ActionLink("Staff Log In", "Login", "PrototypeUI")

<div class="container-fluid">
    <div class="row top-margin">
        <div class="form-group">
            <div class="col-sm-2 col-xs-12 form-top-margin">
                <label>Email</label>
            </div>
            <div class="col-sm-10 col-xs-12 form-top-margin">
                <input type="email" class="form-control" placeholder="hwatson@gmail.com" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-2 col-xs-12 form-top-margin">
                <label>Password</label>
            </div>
            <div class="col-sm-10 col-xs-12 form-top-margin">
                <input type="password" class="form-control" placeholder="********" />
            </div>
        </div>
    </div>
 
    <div class="row">
        <div class="form-group">
            <div class="col-xs-12 form-top-margin">
                <button class="btn btn-block btn-default">Sign In</button>
            </div>
        </div>
    </div>
</div>
