@{
    ViewBag.Title = "ReservationOrder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form class="form-inline">
    <div class="form-group">
        <input type="email" class="form-control" placeholder="Reservation Order #">
    </div>
    <div class="form-group">
        <input type="email" class="form-control" placeholder="RV Site">
    </div>
    <div class="form-group">
        <input type="email" class="form-control" placeholder="Customer Name">
    </div>
    <button type="button" class="btn btn-default"><span class="glyphicon glyphicon-search"></span></button>
</form>
<div class="container row">
    <div class="col-md-1">
        Reservation Order #
    </div>
    <div class="col-md-1">
        Reservation Item #
    </div>
    <div class="col-md-1">
        RV Site
    </div>
    <div class="col-md-2">
        Customer Name
    </div>
    <div class="col-md-1">
        Start Date
    </div>
    <div class="col-md-1">
        End Date
    </div>
    <div class="col-md-1">
        Total
    </div>
</div>
